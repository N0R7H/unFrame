{
    "name": "unFrame",
    "description": "Open a frame or iframe in a new tab or window using a context menu.",
    "version": "1.1.0",
    "minimum_chrome_version": "22.0",
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "16": "img/unFrame16.png",
        "24": "img/unFrame24.png",
        "32": "img/unFrame32.png",
        "48": "img/unFrame48.png",
        "128": "img/unFrame128.png"
    },
    "background": {
        "scripts": [
            "event.js"
        ],
        "persistent": false
    },
    "manifest_version": 2
}
